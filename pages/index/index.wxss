/**index.wxss**/

.top-search {
  background: #ef4836;
  height: 100rpx;
  padding: 20rpx;
  position: relative;
}

.top-search .search-img {
  position: absolute;
  left: 50rpx;
  top: 36rpx;
}

.top-search input {
  width: 570rpx;
  height: 60rpx;
  line-height: 60rpx;
  border: none;
  border-radius: 15px;
  background: #fff;
  display: block;
  padding: 0 0 0 80rpx;
  font-size: 13px;
}

.top-search div {
  width: 80rpx;
  height: 60rpx;
  line-height: 60rpx;
  border: #ef4836 solid 0px;
  color: #fff;
  background: none;
  display: block;
  font-size: 14px;
  text-align: center;
  margin-right: 30rpx;
}

.search-holder {
  font-size: 12px;
  text-indent: 0;
  /* background: url(data:image/png;base64,) no-repeat 10rpx center;
  background-size: 30rpx 30rpx; */
}

.index-banner {
  width: 100%;
  height: 150px;
}

.index-banner image {
  width: 100%;
  height: 150px;
}

.index-tap-l, .index-tap-r {
  width: 375rpx;
  height: 100rpx;
  line-height: 50rpx;
  padding: 30rpx 30rpx 0 30rpx;
  color: #494949;
  text-align: center;
  font-size: 15px;
  border-top: 2px solid #fff;
}

.index-tap-r {
  clear: right;
}

.index-tap-l.current, .index-tap-r.current {
  color: #ef4836;
  border-bottom: 1.5px solid #ef4836;
}

.index-shop-list {
  margin-top: 100rpx;
  overflow: hidden;
  clear: both;
  border-top: 1px solid #f4f4f4;
}

.index-shop-list-item {
  margin-top: 20rpx;
  overflow: hidden;
  clear: both;
}

.shop-name, .shop-instance {
  font-size: 12px;
  color: #828282;
}

.index-start-list image {
  float: right;
  width: 23rpx;
  height: 23rpx;
  margin-left: 5rpx;
  margin-top: 10rpx;
}

.goods-img-warpper {
  width: 300rpx;
  text-align: center;
}

.goods-info-warpper {
  width: 450rpx;
}

.index-goods-img {
  width: 240rpx;
  height: 180rpx;
}

.goods-name {
  font-size: 12px !important;
  margin-top: 15rpx;
  padding-right: 30rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.goods-desc {
  color: #999;
  font-size: 12px !important;
  margin-left: 6px;
  margin-top: 5px;
  padding-right: 30rpx;
}

.goods-item-act {
  margin-top: 15rpx;
  line-height: 36px;
}

.index-goods-down_price {
  width: 80rpx;
  height: 69rpx;
  margin-left: 10rpx;
  /* margin-left: 40%; */
}

.index-goods-price {
  width: 190rpx;
  color: #ef4836;
  font-size: 16px;
  text-align: left;
  margin-left: 10rpx;
}

.index-goods-price text {
  color: #bfbfbf;
  font-size: 12px;
  text-decoration: line-through;
}

.index-goods-buy {
  font-size: 12px;
  width: 120rpx;
  height: 60rpx;
  padding: 0 20rpx;
  line-height: 60rpx;
  border-radius: 4px;
  background: #ef4836;
  color: #fff;
  margin-right: 30rpx;
}

.border {
  border-bottom: 1px solid #bfbfbf;
}

.d-name {
  width: 300rpx;
  color: #000;
  font-size: 13px;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.paading {
  padding: 5px;
}

.shopFooter {
  font-size: 12px;
  color: #666;
  height: 50px;
  line-height: 50px;
  text-align: center;
}
